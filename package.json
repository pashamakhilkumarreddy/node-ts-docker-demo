{
  "name": "node-ts-docker-demo",
  "version": "1.0.0",
  "description": "Sample ",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "clobber": "rm -rf node_modules/ && rm -rf dist/",
    "build": "tsc",
    "dev": "npm run build && node --unhandled-rejections=strict ./dist/app.js",
    "start": "nodemon --watch src/ --ext .ts,.json --exec \"npm run dev && node\"",
    "lint": "eslint src/* --ext .ts",
    "lint:fix": "eslint src/* --ext .ts --fix",
    "debug": "export DEBUG=* && npm run start",
    "prod": "NODE_ENV=production npm run build && node ./dist/app.js",
    "prod-pm2": "NODE_ENV=production npm run build && pm2 start ./dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.0.0",
    "nanoid": "^4.0.2",
    "pm2": "^5.3.0",
    "response-time": "^2.3.2",
    "winston": "^3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pashamakhilkumarreddy/node-ts-docker-demo.git"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "docker"
  ],
  "author": "Pasham Akhil Kumar Reddy <pashamakhilkumarreddy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/response-time": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "source-map-support": "^0.5.21",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  }
}
